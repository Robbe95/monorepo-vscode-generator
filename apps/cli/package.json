{
  "publisher": "robbe95",
  "name": "monorepo-code-generator",
  "displayName": "monorepo-code-generator",
  "type": "module",
  "version": "0.0.3",
  "private": false,
  "packageManager": "pnpm@10.11.0",
  "description": "",
  "author": {
    "name": "Robbe Vaes",
    "email": "robbevaes95@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Robbe95/monorepo-vscode-generator"
  },
  "categories": [
    "Other"
  ],
  "imports": {
    "#*": "./src/*"
  },
  "main": "./dist/index.js",
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown src/index.ts --out-file=dist/index.js --format=esm --watch --inject:./src/shims/node-globals.ts",
    "build": "tsdown src/index.ts --out-file=dist/index.js --format=esm --inject:./src/shims/node-globals.ts",
    "start": "node dist/index.js",
    "meta": "vscode-ext-gen --output src/generated/meta.ts",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "tsdown": "^0.12.6"
  },
  "devDependencies": {
    "@repo/base": "workspace:*",
    "commander": "^14.0.0"
  }
}
