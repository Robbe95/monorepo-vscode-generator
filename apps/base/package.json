{
  "name": "@repo/base",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": "./index.ts"
  },
  "publisher": "robbe95",
  "displayName": "monorepo-code-generator",
  "packageManager": "pnpm@10.11.0",
  "description": "",
  "author": {
    "name": "Robbe Vaes",
    "email": "robbevaes95@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Robbe95/monorepo-vscode-generator"
  },
  "categories": [
    "Other"
  ],
  "imports": {
    "#*": "./src/*"
  },
  "scripts": {
    "build": "tsdown src/index.ts --external vscode",
    "dev": "pnpm build --watch --sourcemap",
    "meta": "vscode-ext-gen --output src/generated/meta.ts",
    "lint": "eslint .",
    "vscode:prepublish": "pnpm build",
    "vscode:publish": "vsce publish --no-dependencies",
    "vscode:pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish"
  },
  "dependencies": {
    "@clack/prompts": "catalog:base"
  },
  "devDependencies": {
    "@antfu/ni": "catalog:monorepo",
    "@types/node": "catalog:type",
    "@types/pluralize": "catalog:type",
    "@types/vscode": "catalog:type",
    "@vscode/vsce": "catalog:vscode",
    "@wisemen/eslint-config-vue": "catalog:lint",
    "bumpp": "catalog:base",
    "eslint": "catalog:lint",
    "esno": "catalog:base",
    "pnpm": "catalog:monorepo",
    "reactive-vscode": "catalog:vscode",
    "tsdown": "catalog:base",
    "typescript": "catalog:base",
    "vite": "catalog:base",
    "vitest": "catalog:base",
    "pluralize": "catalog:base",
    "ts-morph": "catalog:base",
    "vscode-ext-gen": "catalog:vscode"
  }
}
