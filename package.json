{
  "name": "wisemen-code-generator",
  "type": "module",
  "packageManager": "pnpm@10.8.1",
  "version": "0.0.0",
  "license": "MIT",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "clean": "pnpm -r clean",
    "build": "pnpm -r build",
    "typecheck": "turbo typecheck",
    "test": "vitest --run",
    "precommit": "pnpm lint && pnpm typecheck && pnpm test"
  },
  "dependencies": {
    "turbo": "catalog:monorepo"
  },
  "devDependencies": {
    "vitest": "catalog:base"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }

}